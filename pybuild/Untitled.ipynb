    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = np.arange(-0, 10000, 1)\n",
    "\n",
    "expo = int(input(\"select a number to graph: \"))\n",
    "\n",
    "num = x*expo**2\n",
    "\n",
    "plt.ylim(-1000,1000,10)\n",
    "plt.xlim(-1000,1000,10)\n",
    "plt.grid()\n",
    "plt.plot(num)\n",
    "plt.show()"
   ]
